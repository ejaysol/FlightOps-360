        <script><![CDATA[(function executeRule(current, previous) {
    
    if (gs.hasRole('admin') || gs.hasRole('x_1603912_flightop.ops_manager')) {
        return true;
    }
    
    if (gs.hasRole('x_1603912_flightop.crew_member')) {
        var currentUserId = gs.getUserID();
        var scheduledCrewId = current.getValue('crew_member_id');
        return (scheduledCrewId == currentUserId);
    }
    
    return false;
    
})(current, previous);]]></script>
