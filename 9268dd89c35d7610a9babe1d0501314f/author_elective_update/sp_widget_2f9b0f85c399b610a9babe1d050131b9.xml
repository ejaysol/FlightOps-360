        <client_script><![CDATA[function FlightOpsHeaderController($window) {
  var c = this;
  c.handlePrimaryAction = function() {
    if (c.data.primaryCta.link) {
      $window.location = c.data.primaryCta.link;
    }
  };
  c.handleSecondaryAction = function() {
    if (c.data.secondaryCta.link) {
      $window.location = c.data.secondaryCta.link;
    }
  };
}]]></client_script>
        <controller_as>c</controller_as>
        <css>.flightops-header {&#13;
  background: linear-gradient(135deg,#0F62FE,#6C63FF);&#13;
  color: #F4F7FB;&#13;
  padding: 48px 0;&#13;
}&#13;
.flightops-header__eyebrow {&#13;
  text-transform: uppercase;&#13;
  letter-spacing: 0.2em;&#13;
  color: rgba(255,255,255,0.7);&#13;
}&#13;
.flightops-header__title {&#13;
  font-size: 2.5rem;&#13;
  font-weight: 700;&#13;
  margin-bottom: 0.75rem;&#13;
}&#13;
.flightops-header__subtitle {&#13;
  font-size: 1.125rem;&#13;
  max-width: 520px;&#13;
}&#13;
.flightops-header__actions .btn-primary {&#13;
  background: #FFB347;&#13;
  border-color: #FFB347;&#13;
  color: #0A1F44;&#13;
  margin-right: 12px;&#13;
}&#13;
.flightops-header__actions .btn-primary:hover {&#13;
  background: #D7263D;&#13;
  border-color: #D7263D;&#13;
  color: #F4F7FB;&#13;
}&#13;
.flightops-header__image {&#13;
  max-height: 220px;&#13;
}</css>
