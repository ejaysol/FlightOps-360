        <template><![CDATA[<div class="flightops-header" role="banner">
  <div class="container">
    <div class="row align-items-center">
      <div class="col-md-8">
        <p class="flightops-header__eyebrow">{{c.data.eyebrow}}</p>
        <h1 class="flightops-header__title">{{c.data.title}}</h1>
        <p class="flightops-header__subtitle">{{c.data.subtitle}}</p>
        <div class="flightops-header__actions">
          <button class="btn btn-primary" ng-click="c.handlePrimaryAction()">
            {{c.data.primaryCta.label}}
          </button>
          <button class="btn btn-link" ng-click="c.handleSecondaryAction()">
            {{c.data.secondaryCta.label}}
          </button>
        </div>
      </div>
      <div class="col-md-4 text-center">
        <img ng-if="c.data.heroImage" ng-src="{{c.data.heroImage}}" alt="FlightOps illustration" class="img-responsive flightops-header__image" />
      </div>
    </div>
  </div>
</div>]]></template>
