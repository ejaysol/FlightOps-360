        <template><![CDATA[<footer class="flightops-footer" role="contentinfo">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h4>{{c.data.brandName}}</h4>
        <p>{{c.data.tagline}}</p>
        <p class="flightops-footer__contact">
          <i class="fa fa-envelope"></i> {{c.data.email}} &nbsp;
          <i class="fa fa-phone"></i> {{c.data.phone}}
        </p>
      </div>
      <div class="col-md-6 text-md-right">
        <a ng-repeat="link in c.data.links"
           ng-href="{{link.url}}"
           class="flightops-footer__link">
          {{link.label}}
        </a>
        <p class="flightops-footer__copyright">© {{c.data.year}} FlightOps 360. All rights reserved.</p>
      </div>
    </div>
  </div>
</footer>]]></template>
    </sp_widget>
