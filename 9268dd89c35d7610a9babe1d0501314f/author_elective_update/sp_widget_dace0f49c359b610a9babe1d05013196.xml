        <category>custom</category>
        <client_script><![CDATA[function FlightOpsFooterController() {
  var c = this;
}]]></client_script>
        <controller_as>c</controller_as>
        <css>.flightops-footer {&#13;
  background: #0A1F44;&#13;
  color: #F4F7FB;&#13;
  padding: 32px 0;&#13;
}&#13;
.flightops-footer__link {&#13;
  color: #FFB347;&#13;
  margin-left: 16px;&#13;
}&#13;
.flightops-footer__link:hover {&#13;
  color: #D7263D;&#13;
}&#13;
.flightops-footer__contact i {&#13;
  margin-right: 6px;&#13;
}</css>
        <data_table>sp_instance</data_table>
        <demo_data>[{"label":"Privacy","url":"/privacy"},&#13;
 {"label":"Terms","url":"/terms"},&#13;
 {"label":"Status","url":"https://status.flightops360.com"}]</demo_data>
        <description>Branded footer with quick links and contact info</description>
        <docs/>
        <field_list/>
        <has_preview>true</has_preview>
        <id>flightops_footer</id>
        <internal>false</internal>
        <link><![CDATA[function link(scope, element, attrs, controller) {
  
}]]></link>
        <name>FlightOps Footer</name>
        <option_schema>brand_name, tagline, email, phone, links_json</option_schema>
        <public>false</public>
        <roles/>
        <script><![CDATA[(function() {
  data.brandName = options.brand_name || 'FlightOps 360';
  data.tagline = options.tagline || 'AI-enhanced booking & crew operations';
  data.email = options.email || 'support@flightops360.com';
  data.phone = options.phone || '1-800-FLIGHT-360';
  data.year = new GlideDateTime().getYearLocalTime();
  try {
    data.links = JSON.parse(options.links_json || '[]');
  } catch (e) {
    data.links = [];
  }
})();]]></script>
        <servicenow>false</servicenow>
